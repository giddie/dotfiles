[Appearance]
ColorScheme=NeoDark (No Insense)
Font=Bitstream Vera Sans Mono,10,-1,5,50,0,0,0,0,0

[General]
Environment=TERM=konsole-256color,COLORTERM=truecolor
LocalTabTitleFormat=%n %w
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=%u@%H %w

[Interaction Options]
MouseWheelZoomEnabled=false

[Scrolling]
HistoryMode=2
ScrollBarPosition=2

[Terminal Features]
FlowControlEnabled=false
